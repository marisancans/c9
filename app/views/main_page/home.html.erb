<div class="row">
	<div class="col-md-2">
	</div>
	<div class="col-md-10">
		<div class="panel panel-default">
			<div class="panel-body">
				<%= link_to "New question", new_question_path %><br>
				<%= link_to "Questions", questions_path %><br>
				<div id="category-list">
					<% @categories.each do |category| %>
						<%= button_to category, home_new_question_path(category: category), remote: true %>
					<% end %>
				</div>
				<div class="btn-group-vertical" id="current-question-placeholder" role="group">
				</div>
			</div>
		</div>
		----------------------------------------------------------------
		<div id="response"></div>
	</div>
	<div class="col-md-2">
	</div>
</div>



